#to i/p no. day of the week and print name of the day
n = int(raw_input("Enter day number: "))
if(n == 1):
	print "monday"
if(n == 2):
	print "tuesday"
if(n == 3):
	print "wednesday"
if(n == 4):
	print "thursday"
if(n == 5):
	print "friday"
if(n == 6):
	print "saturday"
if(n == 7):
	print "sunday"
else:
	print "Invalid entry!"
